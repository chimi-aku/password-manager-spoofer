<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="416.0" prefWidth="353.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.spoofer.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ImageView fx:id="imageView" fitHeight="194.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />

    <Label fx:id="welcomeText" />
   <Button fx:id="loadImageBtn" mnemonicParsing="false" onAction="#onLoadImageBtnClick" text="LoadImage" />
   <TextField fx:id="outputFileName" onInputMethodTextChanged="#onChangeFileNameTextField" promptText="File" text="File" />
   <Button fx:id="saveFingerprintBtn" mnemonicParsing="false" onAction="#onSaveFingerprintBtnClick" text="Save Fingeprint" />
</VBox>
